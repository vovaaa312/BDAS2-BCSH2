<Window x:Class="VetClinic_DB_APP.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VetClinic_DB_APP.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1400">
    <Grid Margin="0,0,1040,0">
        <Grid.RowDefinitions >
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <TabControl Grid.Row="0" Margin="265,0,-1031,0">
            <TabItem Name="Tab1">
                <TabItem.Header>
                    <TextBlock Text="Text1"/>
                </TabItem.Header>
            </TabItem>
          
            
        </TabControl>
        <ListView Name="TablesListView" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,0,100,0" SelectionChanged="ListView_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
